{
  "name": "@travi/spectacle-scaffolder",
  "description": "scaffolder for presentations using Spectacle",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/spectacle-scaffolder",
  "bugs": "https://github.com/travi/spectacle-scaffolder/issues",
  "homepage": "https://npm.im/@travi/spectacle-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/",
    "templates/"
  ],
  "engines": {
    "node": ">=10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.12.1",
    "@travi/any": "2.0.15",
    "@travi/babel-preset": "3.0.34",
    "@travi/eslint-config": "1.0.56",
    "@travi/eslint-config-mocha": "1.0.8",
    "@travi/javascript-scaffolder": "11.13.1",
    "ban-sensitive-files": "1.9.14",
    "chai": "4.2.0",
    "codecov": "3.8.1",
    "commitlint-config-travi": "1.3.11",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.0",
    "mocha": "8.2.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "9.0.0",
    "remark-preset-lint-travi": "1.3.11",
    "rimraf": "3.0.2",
    "rollup": "2.33.3",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "9.2.0",
    "travis-lint": "1.0.0"
  },
  "dependencies": {
    "@form8ion/cypress-scaffolder": "2.2.0",
    "make-dir": "^3.0.0",
    "mz": "^2.7.0"
  },
  "peerDependencies": {
    "@travi/javascript-scaffolder": "^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0-alpha.1"
  }
}
