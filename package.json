{
  "name": "@travi/spectacle-scaffolder",
  "description": "scaffolder for presentations using Spectacle",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/spectacle-scaffolder",
  "bugs": "https://github.com/travi/spectacle-scaffolder/issues",
  "homepage": "https://npm.im/@travi/spectacle-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/",
    "templates/"
  ],
  "engines": {
    "node": ">=10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "^7.6.2",
    "@travi/any": "^2.0.6",
    "@travi/babel-preset": "^3.0.3",
    "@travi/eslint-config": "1.0.31",
    "@travi/eslint-config-mocha": "1.0.6",
    "@travi/javascript-scaffolder": "^10.0.0",
    "ban-sensitive-files": "^1.9.2",
    "chai": "^4.2.0",
    "codecov": "^3.6.1",
    "commitlint-config-travi": "^1.3.4",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^4.0.2",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-travi": "^1.2.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.24.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^8.0.0",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "@form8ion/cypress-scaffolder": "2.1.0",
    "make-dir": "^3.0.0",
    "mz": "^2.7.0"
  },
  "peerDependencies": {
    "@travi/javascript-scaffolder": "^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0"
  }
}
